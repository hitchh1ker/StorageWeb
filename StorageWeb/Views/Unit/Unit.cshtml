@{

Layout = "~/Views/Shared/_Layout.cshtml";
@using StorageWeb.Repository.Unit.Models
@model IEnumerable<Unit>

<h2>Единицы измерения</h2>

<table border="1" cellpadding="10">
    <thead>
        <tr>
            <th>Название</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var unit in Model)
        {
            <tr>
                <td>@unit.Name</td>
            </tr>
        }
    </tbody>
</table>
}
