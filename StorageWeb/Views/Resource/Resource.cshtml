@{

Layout = "~/Views/Shared/_Layout.cshtml";
@using StorageWeb.Repository.Resource.Models
@model IEnumerable<Resource>

<h2>Ресурсы</h2>

<table border="1" cellpadding="10">
    <thead>
        <tr>
            <th>Название</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var resource in Model)
        {
            <tr>
                <td>@resource.Name</td>
            </tr>
        }
    </tbody>
</table>
}
